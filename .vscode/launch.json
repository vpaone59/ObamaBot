{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debug",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/app/main.py",
            "autoReload": {
                "enable": true
            },
            "cwd": "${workspaceFolder}/",
            "console": "integratedTerminal",
            "justMyCode": false
        }
    ]
}